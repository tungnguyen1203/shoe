<tr id='category_<%= list_category.id %>' >
  <th scope="row"><%= list_category.id %></th>
  <td><%= list_category.name %></td>
  <td >
    <%=link_to  admin_category_path(list_category), method: :delete,data: { confirm: "bạn chắc chưa?" }, class:"btn btn-outline-danger", remote: true do %>
    <i class="fa-solid fa-trash-can"></i>
    <% end %>
    <%=link_to  edit_admin_category_path(list_category), class:"btn btn-outline-warning" do %>
    <i class="fa-solid fa-pen-to-square"></i>
    <% end %>
  </td>
</tr>
